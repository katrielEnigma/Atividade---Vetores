public class atividade02 {
    public static void main(String[] args) {
        // Declaração dos vetores
        int[] vetor = {3, 6, 9, 12};

        // calcular a soma dos vetores
        int soma = vetor[0] + vetor[3];


        System.out.println("A soma do primeiro e último elemento é: " + soma);
    }
}
